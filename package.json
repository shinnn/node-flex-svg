{
  "name": "flex-svg",
  "version": "2.1.1",
  "description": "Create SVG files of flexible width and height",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json cli.js index.js test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/node-flex-svg",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "keywords": [
    "cli",
    "command-line",
    "svg",
    "xml",
    "responsive",
    "flexible",
    "width",
    "height"
  ],
  "license": "MIT",
  "dependencies": {
    "get-stdin": "^4.0.1",
    "output-file-sync": "^1.1.1",
    "xml2js": "^0.4.9",
    "yargs": "^3.14.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "eslint": "^0.24.0",
    "istanbul": "^0.3.17",
    "istanbul-coveralls": "^1.0.3",
    "read-remove-file": "^2.0.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  }
}
