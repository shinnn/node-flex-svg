{
  "name": "flex-svg",
  "version": "2.1.1",
  "description": "Create SVG files of flexible width and height",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "pretest": "eslint --fix --format=codeframe cli.js index.js test/test.js",
    "test": "node test/test.js",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/node-flex-svg",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "keywords": [
    "cli",
    "command-line",
    "svg",
    "xml",
    "responsive",
    "flexible",
    "width",
    "height"
  ],
  "license": "MIT",
  "dependencies": {
    "get-stdin": "^5.0.1",
    "output-file-sync": "^2.0.0",
    "xml2js": "^0.4.19",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@shinnn/eslint-config-cli": "^3.0.0",
    "eslint": "^4.9.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "read-remove-file": "^5.0.0",
    "tape": "^4.8.0"
  },
  "eslintConfig": {
    "extends": "@shinnn/cli"
  }
}
