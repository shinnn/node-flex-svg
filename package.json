{
  "name": "flex-svg",
  "version": "2.0.0",
  "description": "Create SVG files of flexible width and height",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "jscs *.js test/test.js && eslint *.js test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/node-flex-svg",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "keywords": [
    "cli",
    "command-line",
    "svg",
    "xml",
    "responsive",
    "flexible",
    "width",
    "height"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/node-flex-svg/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "get-stdin": "^3.0.2",
    "output-file-sync": "^1.1.0",
    "xml2js": "^0.4.4",
    "yargs": "^1.3.3"
  },
  "devDependencies": {
    "eslint": "^0.11.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "read-remove-file": "^1.0.1",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
